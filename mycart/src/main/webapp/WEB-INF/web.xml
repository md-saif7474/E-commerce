<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterSevlet</servlet-name>
  	<display-name>RegisterSevlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.RegisterSevlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.LoginServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>LogoutSevlet</servlet-name>
  	<display-name>LogoutSevlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.LogoutSevlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductOperationServlet</servlet-name>
  	<display-name>ProductOperationServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.ProductOperationServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>RegisterSevlet</servlet-name>
  	<url-pattern>/RegisterSevlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>LogoutSevlet</servlet-name>
  	<url-pattern>/LogoutSevlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductOperationServlet</servlet-name>
  	<url-pattern>/ProductOperationServlet</url-pattern>
  </servlet-mapping>
</web-app>
